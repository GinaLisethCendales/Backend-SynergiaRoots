
<% for (let i = 0; i < users.length; i++) { %>
    <tr id="<%= users[i]._id %>">
        <td class="item-table" id="item-nombre"><%= users[i].username %></td>
        <td class="item-table"><%= users[i].email %></td>
        <td class="item-table"><%= users[i].role %></td>
        <td class="item-table"><%= users[i].createdAt %></td>
        <td> 
            <button class="editar" onclick="editar('<%= users[i]._id %>')"><i class="fa-solid fa-lg fa-pen-to-square" style="color:green;"></i></button>
            <button class="eliminar" onclick="eliminar('<%= users[i]._id %>')"><i class="fa-solid fa-lg fa-trash" style="color:red;"></i></button>
        </td>
    </tr>
<% } %>
